{
  "name": "@linktr.ee/statsig-react",
  "version": "1.34.0-alpha.96",
  "description": "An SDK for using Statsig Feature Management and Experimentation platform in React clients",
  "type": "module",
  "main": "index.js",
  "types": "index.d.ts",
  "scripts": {
    "prepare": "rm -rf dist/ && genversion src/SDKVersion.ts --es6 && tsc",
    "test": "jest --verbose",
    "publish-custom-version": "node ./publish.cjs",
    "styleguide": "npm run eslint && npm run format:check",
    "styleguide:fix": "npm run eslint:fix && npm run format:fix",
    "format:check": "prettier -l --ignore-path .gitignore './**/*.{html,js,json,md,ts,tsx,yaml}'",
    "format:fix": "npm run format:check -- --write",
    "eslint": "eslint './src/**/*.{ts,tsx}'",
    "eslint:fix": "npm run eslint -- --fix"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/blstrco/statsig-react-sdk.git"
  },
  "keywords": [
    "Feature",
    "Flag",
    "Management",
    "A/B",
    "Test"
  ],
  "author": "Statsig, Inc & Linktree",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/blstrco/statsig-react-sdk/issues"
  },
  "homepage": "https://github.com/blstrco/statsig-react-sdk#readme",
  "devDependencies": {
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^12.1.5",
    "@testing-library/user-event": "^14.4.3",
    "@types/jest": "^27.0.2",
    "@types/node": "^16.7.9",
    "@types/react": "^17.0.9",
    "@types/react-dom": "^17.0.9",
    "@typescript-eslint/eslint-plugin": "^5.59.7",
    "@typescript-eslint/parser": "^5.59.7",
    "eslint": "^8.41.0",
    "genversion": "^3.1.1",
    "jest": "^27.2.5",
    "prettier": "^3.1.1",
    "react-dom": "^16.8.4 || ^17.0.0 || ^18.0.0",
    "ts-jest": "^27.0.5",
    "typescript": "^4.3.2"
  },
  "dependencies": {
    "statsig-js": "^4.45.1"
  },
  "peerDependencies": {
    "react": "^16.6.3 || ^17.0.0 || ^18.0.0"
  }
}
